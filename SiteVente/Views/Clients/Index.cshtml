<table class="table">
	<thead>
		<tr>
			<th>
				Nom
			</th>
			<th>
				Type du Client
			</th>
			<th>
				Adresse
			</th>
			<th>
				Modifier
			</th>
			<th>
				Supprimer
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach(Client p in Model)
		{
			<tr>
				<td>
					@p.Nom
				</td>
				<td>
					@p.TypeClient
				</td>
				<td>
					@p.Adresse
				</td>
				<td><a class="btn btn-primary" asp-action="Edit" asp-route-id="@p.Nom">Modifier</a></td>
				<td><a class="btn btn-danger" asp-action="Delete" asp-route-id="@p.Nom">Delete</a></td>
		</tr>
		}
	</tbody>
</table>